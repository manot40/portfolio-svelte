{
  "useTabs": false,
  "singleQuote": true,
  "endOfLine": "lf",
  "arrowParens": "always",
  "bracketSpacing": true,
  "trailingComma": "none",
  "printWidth": 120,
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ]
}
